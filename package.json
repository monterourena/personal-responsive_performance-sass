  "bugs": {
    "url": "https://github.com/monterourena/performance-optimization-responsive-design-sass/issues"
  },
  "homepage": "https://github.com/monterourena/performance-optimization-responsive-design-sass#readme",
  "devDependencies": {
    "node-sass": "^8.0.0"
  }
}
